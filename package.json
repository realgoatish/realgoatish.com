{
	"name": "realgoatish.com",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run start && vite build",
		"preview": "npm run start && vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"rimraf": "rimraf temp",
		"imageminsvgo": "node imagemin-svgo.js",
		"prebuildsprite": "npm run imageminsvgo",
		"buildsprite": "svgstore -o static/svg/sprite/sprite.svg temp/svg/*.svg --inline",
		"embedsprite": "node embed-sprite.js",
		"start": "npm run buildsprite && npm run embedsprite && npm run rimraf"
	},
	"devDependencies": {
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"imagemin": "^8.0.1",
		"imagemin-svgo": "^10.0.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rimraf": "^3.0.2",
		"svelte": "^3.46",
		"svelte-check": "^2.2.6",
		"svelte-dts": "^0.3.7",
		"svelte-preprocess": "^4.9.4",
		"svgstore": "^3.0.1",
		"svgstore-cli": "^2.0.1",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"vite": "^2.9.13"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/playfair-display": "^4.5.1",
		"@prismicio/client": "^5.1.0",
		"@realgoatish/svelte-every-layout": "0.0.19",
		"dotenv": "^10.0.0",
		"prism-svelte": "^0.5.0",
		"prismic-dom": "^2.2.6",
		"prismjs": "^1.27.0"
	}
}
